<!DOCTYPE html>
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>Notícias</title>
		</head>
	<body>

		<h1>Notícias</h1>
		<table>
			<thead>
				<tr>
					<th>ID</th>
					<th>Titulo</th>
					<th>Noticia</th>
				</tr>
			</thead>
			<tbody>
				<% for(var i=0; i< noticias.length; i++){ %>
				<tr>
					<td><%= noticias[i].id %></td>
					<td><%= noticias[i].titulo %></td>
					<td><%= noticias[i].noticia %></td>
				</tr>
				<% } %>
			</tbody>
		</table>
	</body>
</html>